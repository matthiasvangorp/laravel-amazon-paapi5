{
    "name": "matthiasvangorp/laravel-amazon-paapi5",
    "description": "Laravel package for Amazon Product advertising API v5 using Official PHP SDK",
    "version" : "1.0.5",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Matthias Van Gorp",
            "email": "matthiasvangorp@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "MatthiasVanGorp\\LaravelAmazonPaapi5\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MatthiasVanGorp\\LaravelAmazonPaapi5\\LaravelAmazonPaapi5ServiceProvider"
            ],
            "aliases": {
                "AmazonPaapi5": "MatthiasVanGorp\\LaravelAmazonPaapi5\\Facades\\AmazonAPI"
            }
        }
    },
    "require": {
        "php" : ">=8.0",
        "guzzlehttp/guzzle" : "^7.0"
    }
}
